{
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=16"
    },
    "name": "@khanacademy/wonder-stuff-render-environment-jsdom",
    "version": "1.0.4",
    "description": "Render environment to use with the Wonder Stuff Render Server",
    "module": "dist/es/index.js",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "example": "bash -c 'RUNNER=\"./examples/$0/run.ts\"; KA_LOG_LEVEL=silly NODE_ENV=development babel-watch --config-file ../../babel.config.js \"$RUNNER\" --extensions .ts'",
        "test": "bash -c 'yarn --silent --cwd \"../..\" test ${@:0} $($([[ ${@: -1} = -* ]] || [[ ${@: -1} = bash ]]) && echo $PWD)'"
    },
    "dependencies": {
        "@khanacademy/wonder-stuff-core": "^1.4.2",
        "@khanacademy/wonder-stuff-server": "^4.0.6",
        "@khanacademy/wonder-stuff-render-server": "^1.0.4"
    },
    "devDependencies": {
        "ws-dev-build-settings": "^1.1.0"
    },
    "peerDependencies": {
        "jsdom": "^21.1.1"
    },
    "author": "",
    "license": "MIT"
}